	keywords = {Thyrotropin, Central hyperthyroidism, Resistance to thyroid hormones, Somatostatin analogs, Thyrotropin-secreting pituitary adenoma, Transsphenoidal surgery},
	pages = {76--82},
	file = {PDF:/Users/ville/Zotero/storage/9TZGLXLZ/2013-2013_European_thyroid_association_guidelines_for_the_diagnosis_and_treatment_of_thyrotropin-secreting_pituitary_tum.pdf:application/pdf},
