	pmid = {12602570},
	keywords = {Female, Humans, Male, Aged, Cohort Studies, Smoking, Aging, Social Support, Educational Status, Health Status Indicators, Housing for the Elderly, Institutionalization, Marital Status},
	pages = {361--370},
}

