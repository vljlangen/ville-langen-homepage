	keywords = {Female, Finland, Humans, Longitudinal Studies, Male, Cardiovascular Diseases, Aged, Cohort Studies, Cholesterol, Proportional Hazards Models, Cause of Death, Triglycerides, Lipids, Hypercholesterolemia, Prospective Studies, Mortality, Cholesterol, HDL},
