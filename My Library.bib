	keywords = {Female, Humans, Male, Middle Aged, Adult, Blood Pressure Monitoring, Ambulatory, Circadian Rhythm/physiology, Hypertension/physiopathology, Hypothyroidism/physiopathology, Thyrotropin/physiology},
