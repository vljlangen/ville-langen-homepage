	file = {PDF:/Users/ville/Zotero/storage/G92GUH23/Zava, Zava - 2011 - Assessment of Japanese iodine intake based on seaweed consumption in Japan A literature-based analysis. - Thyroid re.pdf:application/pdf},
}

