	author = {Andreotti, Felicita and Rocca, Bianca and Husted, Steen and Ajjan, Ramzi A. and Berg, Jurrien Ten and Cattaneo, Marco and Collet, Jean Philippe and Caterina, Raffaele De and Fox, Keith A.A. and Halvorsen, Sigrun and Huber, Kurt and Hylek, Elaine M. and Lip, Gregory Y.H. and Montalescot, Gilles and Morais, Joao and Patrono, Carlo and Verheugt, Freek W.A. and Wallentin, Lars and Weiss, Thomas W. and Storey, Robert F.},
	year = {2015},
	pages = {3238--3249},
	file = {PDF:/Users/ville/Zotero/storage/S9I97U8I/Andreotti et al. - 2015 - Antithrombotic therapy in the elderly Expert position paper of the European society of cardiology working grou.pdf:application/pdf},
