	note = {Publisher: Sociedade Portuguesa de Cardiologia},
	keywords = {Heart failure, Fração de ejeção reduzida, Insuficiência cardíaca, Medical therapy, Reduced ejection fraction, Sacubitril-valsartan, Sacubitril/valsartan, Terapêutica médica},
	pages = {491--496},
