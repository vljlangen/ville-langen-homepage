	pages = {498--503},
	file = {PDF:/Users/ville/Zotero/storage/A9M4IZ24/Pachucki, Burmeister, Larsen - 1999 - Thyroid hormone regulates hyperpolarization-activated cyclic nucleotide-gated channel (HCN2) mRNA.pdf:application/pdf},
