	note = {Place: Karolinska Institute, Dept of Medicine, Huddinge University Hospital, Sweden.},
	keywords = {Humans, Animals, Cells, Cultured, Thyrotropin/pharmacology, Mice, Analysis of Variance, Cell Line, Blotting, Northern, Kinetics, Gene Expression/drug effects, Heart Neoplasms/metabolism, Liver/metabolism, Mice, Inbred Strains, Myocardium/metabolism, Organ Specificity, Polymerase Chain Reaction/methods, Receptors, Thyrotropin/analysis/biosynthesis, RNA, Messenger/analysis/biosynthesis, Tumor Cells, Cultured},
