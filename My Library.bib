	keywords = {Female, Finland, Humans, Male, Risk Factors, Cardiovascular Diseases, Aged, Cohort Studies, Cross-Sectional Studies, Cholesterol, Aging, Aged, 80 and over, Prospective Studies, Cholesterol, LDL, Insulin, Cholesterol, HDL},
