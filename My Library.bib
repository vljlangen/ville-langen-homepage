@article{lazzarini_heart_2013,
	title = {Heart failure in elderly patients: {Distinctive} features and unresolved issues},
	volume = {15},
	issn = {13889842},
	doi = {10.1093/eurjhf/hft028},
	abstract = {The prevalence of heart failure (HF) increases with age. While clinical trials suggest that contemporary evidence-based HF therapies have reduced morbidity and mortality, these trials largely excluded the elderly. Questions remain regarding the clinical characteristics of elderly HF patients and the impact of contemporary therapies on their outcomes. This review presents the epidemiology of HF in the elderly and summarizes the data on the pathophysiology of the ageing heart. The clinical characteristics, treatment patterns, and outcomes of elderly HF patients are explored. Finally, the main gaps regarding HF therapies in the elderly and the opportunities for future trials are highlighted. © 2013 The Author.},
