	keywords = {Female, Humans, Male, Aged, Hospitalization, Risk, Aged, 80 and over, Disease, Prospective Studies, Severity of Illness Index, Activities of Daily Living, Disabled Persons, Frail Elderly, Connecticut, Wounds and Injuries},
