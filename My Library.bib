	keywords = {Female, Humans, Longitudinal Studies, Male, Middle Aged, Adult, Follow-Up Studies, Adolescent, Aging, Child, Social Environment, Personal Satisfaction, Interpersonal Relations, Parent-Child Relations, Personality Development, Sibling Relations, Social Support},
