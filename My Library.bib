	pages = {411--4},
	file = {PDF:/Users/ville/Zotero/storage/3KW8GJ8S/Lekakis et al. - 1997 - Flow-mediated, endothelium-dependent vasodilation is impaired in subjects with hypothyroidism, borderline hypoth.pdf:application/pdf},
