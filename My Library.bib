	journal = {Journal of Human Hypertension},
	author = {Jiang, F. and Liu, A. and Lai, Y. and Yu, X. and Li, C. and Han, C. and Zhang, Y. and Wang, X. and Wang, Z. and Bao, S. and Lv, N. and Jin, M. and Yang, F. and Fan, Y. and Jin, T. and Zhao, W. and Shan, Z. and Teng, W.},
	year = {2017},
	pages = {244--247},
	file = {PDF:/Users/ville/Zotero/storage/A3NTFUAK/Jiang et al. - 2017 - Change in serum TSH levels within the reference range was associated with variation of future blood pressure A 5-y.pdf:application/pdf},
}

