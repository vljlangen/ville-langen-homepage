	keywords = {Female, Finland, Humans, Male, Aged, Cause of Death, Prognosis, Survival, Aged, 80 and over, Personal Satisfaction, Depression, Surveys and Questionnaires, Attitude, Loneliness, Value of Life},
	pages = {133--137},
}

