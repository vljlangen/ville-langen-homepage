	keywords = {Female, Finland, Humans, Male, Aged, Cohort Studies, Aging, Aged, 80 and over, Quality of Life, Health Status, Regression Analysis, Activities of Daily Living, Surveys and Questionnaires, Loneliness, Cohort comparison, Older people},
	pages = {254--260},
