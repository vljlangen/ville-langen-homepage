	keywords = {Female, Humans, Male, Aged, Heart Failure, Myocardial Infarction, Length of Stay, Patient Readmission, Hospital Mortality, Gastrointestinal Hemorrhage, Medicare, Ohio, Pneumonia, Pulmonary Disease, Chronic Obstructive, Resuscitation Orders},
